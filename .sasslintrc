{
  "files": {
    "include": "src/style/**/*.scss",
    "ignore": [
      "src/style/vendor/**/*.*"
    ]
  },
  "rules": {
    "force-element-nesting": 0,
    "no-url-domains": 0,
    "no-url-protocols": 0,
    "force-pseudo-nesting": 0,
    "mixins-before-declarations": 0,
    "no-ids": 0,
    "class-name-format": 0
  }
}